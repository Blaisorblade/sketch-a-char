<!DOCTYPE html>
<html>
<head>
  <title>Sketch-A-Char</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag 'underscore-min.js' %>
  <%= javascript_include_tag 'underscore-mixins.js' %>
  <%= javascript_include_tag 'raphael-min.js' %>
  <%= javascript_include_tag 'jquery.raphael.canvassify.js' %>
  <%= csrf_meta_tag %>
</head>
<body>
  
  <% if signed_in? %><small>Signed in as <%= current_user.name %></small><% else %><small><%= link_to 'Login', login_pathg %></small><% end %>

  <%= yield %>

</body>
</html>
